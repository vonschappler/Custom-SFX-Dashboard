{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "[Custom SFX v2.0.0] sound effects visual controller.",
  "main": "src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder -w",
    "build": "electron-builder build --publish never",
    "deploy": "electron-builder build --win --publish always"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vonschappler/Custom-SFX-Dashboard.git"
  },
  "author": {
    "name": "von Schappler",
    "email": "vonschappler.gaming@gmail.com",
    "url": "http://rebrand.ly/vonWebsite"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vonschappler/Custom-SFX-Dashboard/issues"
  },
  "homepage": "https://github.com/vonschappler/Custom-SFX-Dashboard#readme",
  "dependencies": {
    "electron-log": "^4.4.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^4.3.9",
    "jquery": "^3.6.0",
    "moment": "^2.29.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@electron-forge/maker-deb": "^6.0.0-beta.57",
    "@electron-forge/maker-rpm": "^6.0.0-beta.57",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.57",
    "@electron-forge/maker-zip": "^6.0.0-beta.57",
    "electron": "13.1.4",
    "electron-builder": "^22.11.7"
  },
  "build": {
    "win": {
      "target": "nsis-web",
      "executableName": "dashboard",
      "publisherName": "",
      "requestedExecutionLevel": "requireAdministrator",
      "icon": "./src/img/assets/dashboard.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true
    }
  }
}
